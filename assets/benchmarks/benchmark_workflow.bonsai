            <Expression xsi:type="p1:CreateMessage">
              <harp:Payload xsi:type="p1:CreateSetSpeedPayload">
                <harp:MessageType>Write</harp:MessageType>
                <p1:Value>-100</p1:Value>
              </harp:Payload>
            </Expression>
